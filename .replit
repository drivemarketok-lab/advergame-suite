run = "npm run dev"
entrypoint = "index.js"
hidden = [".config", "package-lock.json", ".next", ".swc"]
modules = ["nodejs-22"]

[nix]
channel = "stable-24_11"
packages = ["psmisc", "imagemagick_light"]

[env]
NEXT_TELEMETRY_DISABLED = "1"

[deployment]
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 3005
externalPort = 3000

[[ports]]
localPort = 3010
externalPort = 3002

[[ports]]
localPort = 3020
externalPort = 3003

[[ports]]
localPort = 3021
externalPort = 4200

[agent]
expertMode = true
